/* Tooltip container */
.tooltip-new {
  position: relative;
  display: flex;
  opacity: 1;
	padding: 0;
	user-select: none;
	z-index: 3;
}

/* Tooltip text */
.tooltip-new .tooltiptext {
  visibility: hidden;
  width: 240px;
  background-color: #44813c;
  border: solid 1px black;
  color: white;
  text-align: center;
  padding: 5px 8px;
  border-radius: 6px;
  bottom: 150%;
  left: 50%; 
  margin-left: -126px; /* Use half of the width (120/2 = 60), to center the tooltip */
 
  /* Position the tooltip text - see examples below! */
  position: absolute;
  z-index: 1;
}
  
.tooltip-new .tooltiptext::after {
  content: " ";
  position: absolute;
  top: 100%; /* At the bottom of the tooltip */
  left: 50%;
  margin-left: -5px;
  border-width: 5px;
  border-style: solid;
  border-color: black transparent transparent transparent;
}

/* Show the tooltip text when you mouse over the tooltip container */
.tooltip-new span:hover + .tooltiptext {
  visibility: visible;
}
  
.cbx {
	margin: 0 .7em 0 0;
  -webkit-user-select: none;
  user-select: none;
}
.cbx span {
  display: inline-block;
  vertical-align: middle;
	padding-left: 0.7em;
	font-weight: 400;
}
.insurance-popup {
	font-weight: 600;
	color: #44813c;
}
.cbx span:first-child {
  position: relative;
  width: 18px;
  height: 18px;
  border-radius: 3px;
  transform: scale(1);
  vertical-align: middle;
  border: 1px solid #9098A9;
  transition: all 0.2s ease;
	align-self: center;
  cursor: pointer;
}
.cbx span:first-child svg {
  position: absolute;
  top: 3px;
  left: 2px;
  fill: none;
  stroke: #FFFFFF;
  stroke-width: 2;
  stroke-linecap: round;
  stroke-linejoin: round;
  stroke-dasharray: 16px;
  stroke-dashoffset: 16px;
  transition: all 0.3s ease;
  transition-delay: 0.1s;
  transform: translate3d(0, 0, 0);
}
.cbx span:first-child:hover  {
  border-color: #44813c;
}

.inp-cbx:checked + .cbx span:first-child {
  background: #44813c;
  border-color: #44813c;
  animation: wave 0.4s ease;
}
.inp-cbx:checked + .cbx span:first-child svg {
  stroke-dashoffset: 0;
}
.inp-cbx:checked + .cbx span:first-child:before {
  transform: scale(3.5);
  opacity: 0;
  transition: all 0.6s ease;
}

@keyframes wave {
  50% {
    transform: scale(0.9);
  }
}

